      "candid": "src/genesisnet_backend_backend/genesisnet_backend_backend.did",
      "build": "npx azle genesisnet_backend_backend",
      "wasm": ".azle/genesisnet_backend_backend/genesisnet_backend_backend.wasm",
      "gzip": true,
      "tech_stack": {
        "cdk": { "azle": {} },
